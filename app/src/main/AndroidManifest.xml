<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="projtetst.shaheen.com.pro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.shaheen_mac.project.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.shaheen_mac.project.SignupActtivity"
            android:label="@string/title_activity_signup_acttivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.shaheen_mac.project.ShopActivity"
            android:label="Shop"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.shaheen_mac.project.dispctivity"
            android:label="Disp"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
